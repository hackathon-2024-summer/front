{
  "name": "hackathon_2024_summer_front",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "reactapp",
  "workspaceFolder": "/home/appuser/front",
  "shutdownAction": "stopCompose",
  "remoteUser": "appuser",
  "updateRemoteUserUID": true,
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "mhutchie.git-graph",
        "dbaeumer.vscode-eslint",
        "bradlc.vscode-tailwindcss"
      ]
    }
  }
  // "postCreateCommand": "sh -c 'curl -fsSL https://raw.githubusercontent.com/ohmyzsh/ohmyzsh/master/tools/install.sh | sh -s -- --unattended && echo \"source $HOME/.oh-my-zsh/oh-my-zsh.sh\" >> ~/.zshrc'",
}